{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            cp.*,\n            u.name as author_name,\n            u.username as author_username,\n            u.avatar as author_avatar\n        FROM creator_posts cp\n        JOIN users u ON cp.author_id = u.id\n        WHERE cp.author_id = $1\n        ORDER BY cp.created_at DESC\n        LIMIT $2 OFFSET $3\n        ",
