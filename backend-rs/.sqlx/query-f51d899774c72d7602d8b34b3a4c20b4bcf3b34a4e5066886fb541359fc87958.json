  "query": "\n        SELECT c.*, u.name as user_name, u.avatar as user_avatar\n        FROM comments c\n        JOIN users u ON c.user_id = u.id\n        WHERE c.campaign_id = $1 AND c.parent_id IS NULL\n        ORDER BY c.created_at DESC\n        ",
