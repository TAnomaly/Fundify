  "query": "\n            SELECT \n                g.*,\n                u.name as creator_name,\n                u.avatar as creator_avatar\n            FROM goals g\n            JOIN users u ON g.creator_id = u.id\n            WHERE g.creator_id = $1 AND g.status = $2\n            ",
