  "db_name": "PostgreSQL",
  "query": "\n        SELECT m.*, u.name as creator_name, u.avatar as creator_avatar\n        FROM messages m\n        JOIN users u ON m.creator_id = u.id\n        WHERE m.creator_id = $1 AND m.message_type = 'BROADCAST'\n        ORDER BY m.created_at DESC\n        LIMIT $2 OFFSET $3\n        ",
