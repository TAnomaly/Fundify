{
  "name": "fundify-backend",
  "version": "1.0.2",
  "description": "Backend API for Fundify crowdfunding platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "npx prisma generate && ./node_modules/.bin/tsc",
    "lint": "eslint . --ext .ts",
    "start": "node dist/index.js",
    "deploy": "node scripts/deploy.js",
    "start:with-migration": "echo 'Starting migration...' && npx prisma db push --accept-data-loss --force-reset && echo 'Migration completed!' && npm start",
    "db:migrate": "npx prisma migrate dev",
    "db:deploy": "npx prisma migrate deploy",
    "db:push": "npx prisma db push --accept-data-loss",
    "db:reset": "npx prisma migrate reset",
    "db:studio": "npx prisma studio",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "crowdfunding",
    "express",
    "prisma",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "5.22.0",
    "@supabase/supabase-js": "^2.75.0",
    "@types/multer": "^2.0.0",
    "amqplib": "^0.10.4",
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.41.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "node-cron": "^4.2.1",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "stripe": "^19.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@railway/cli": "^4.10.0",
    "@types/amqplib": "^0.10.5",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "@types/passport": "^1.0.17",
    "@types/passport-github2": "^1.2.9",
    "@eslint/eslintrc": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "nodemon": "^3.0.2",
    "prisma": "5.22.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}